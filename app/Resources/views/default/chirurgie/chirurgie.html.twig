{% extends 'base.html.twig' %}

{% block body %}
    <!-- Start Preloader -->
    <div id="tm-preloader">
        <div id="tm-preloader-in">
            <div></div><div></div><div></div><div></div><div></div>
        </div>
    </div>
    <!-- End Preloader -->

    {% include 'default/header1.html.twig' %}

    <!-- Start Blog Header -->
    <section class="tm-blog-hero" style="background-image: url('{{ vich_uploader_asset(chirurgie,'backimageFile') }}')">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="tm-blog-hero-title">
                        {% if app.request.locale =='fr' %}
                            {{ chirurgie.name }}
                        {% else  %}
                            {{ chirurgie.nameen }}
                        {% endif %}</h1>
                    <ul class="tm-breadcrumb">
                        <li><a href="{{ path('homepage') }}">Home</a></li>
                        <li>
                        {% if app.request.locale =='fr' %}
                            {{ chirurgie.name }}
                        {% else  %}
                            {{ chirurgie.nameen }}
                        {% endif %}
                        </li>
                    </ul>
                </div><!-- .col -->
            </div>
        </div>
    </section>
    <!-- End Blog Header -->

    <!-- Start Site Content -->
    <div class="container justify-text">
        <div class="row justify-content-center p-5">
            {% if(chirurgie.details) %}
            <div class="col-lg-10 col-md-10">
                {% if app.request.locale =='fr' %}
                    {{ chirurgie.details|raw }}
                {% else  %}
                    {{ chirurgie.detailsen|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.baimage) %}
            <div class="row justify-content-center col-lg-8 col-md-8 p-5">
                <img src="{{ vich_uploader_asset(chirurgie,'baimageFile') }}" alt="">
            </div>
            {% endif %}

            {% if(chirurgie.details1) %}
            <div class="col-lg-10 col-md-10 pt-5">
                {% if app.request.locale =='fr' %}
                    {{ chirurgie.details1|raw }}
                {% else  %}
                    {{ chirurgie.details1en|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.detailsimage) %}
            <div class="row justify-content-center col-lg-8 col-md-8 p-5">
                <img src="{{ vich_uploader_asset(chirurgie,'detailsimageFile') }}" alt="">
            </div>
            {% endif %}

            {% if(chirurgie.details2) %}
            <div class="col-lg-10 col-md-10 pt-5">
                {% if app.request.locale =='fr' %}
                    {{ chirurgie.details2|raw }}
                {% else  %}
                    {{ chirurgie.details2en|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.beforeOperation) %}
            <div class="col-lg-10 col-md-10 pt-5">

                {% if app.request.locale =='fr' %}
                    {{ chirurgie.beforeOperation|raw }}
                {% else  %}
                    {{ chirurgie.beforeOperationen|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.detailsimage2) %}
            <div class="row justify-content-center col-lg-8 col-md-8 p-5">
                <img src="{{ vich_uploader_asset(chirurgie,'detailsimageFile2') }}" alt="">
            </div>
            {% endif %}

            {% if(chirurgie.beforeOperation1) %}
            <div class="col-lg-10 col-md-10 pt-5">

                {% if app.request.locale =='fr' %}
                    {{ chirurgie.beforeOperation1|raw }}
                {% else  %}
                    {{ chirurgie.beforeOperation1en|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.detailsimage3) %}
            <div class="row justify-content-center col-lg-8 col-md-8 p-5">
                <img src="{{ vich_uploader_asset(chirurgie,'detailsimageFile3') }}" alt="">
            </div>
            {% endif %}

            {% if(chirurgie.afterOperation) %}
            <div class="col-lg-10 col-md-10 pt-5">

                {% if app.request.locale =='fr' %}
                    {{ chirurgie.afterOperation|raw }}
                {% else  %}
                    {{ chirurgie.afterOperationen|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.afterOperation1) %}
            <div class="col-lg-10 col-md-10 pt-5">

                {% if app.request.locale =='fr' %}
                    {{ chirurgie.afterOperation1|raw }}
                {% else  %}
                    {{ chirurgie.afterOperation1en|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.complications) %}
            <div class="col-lg-10 col-md-10 pt-5">

                {% if app.request.locale =='fr' %}
                    {{ chirurgie.complications|raw }}
                {% else  %}
                    {{ chirurgie.complicationsen|raw }}
                {% endif %}
            </div>
            {% endif %}

            {% if(chirurgie.complications1) %}
            <div class="col-lg-10 col-md-10 pt-5">

                {% if app.request.locale =='fr' %}
                    {{ chirurgie.complications1|raw }}
                {% else  %}
                    {{ chirurgie.complications1en|raw }}
                {% endif %}
            </div>
            {% endif %}

            {#
             {% if(chirurgie.tarifs) %}
            <div class="col-lg-10 col-md-10 pt-5">
                {{ chirurgie.tarifs|raw }}
            </div>
            {% endif %}
            #}


        </div>
    </div>
    <!-- End Site Content -->

    {% include 'default/footer.html.twig' %}

    <!-- Scroll Up -->
    <div id='scrollup'></div>
{% endblock %}